<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IService1">
          <security>
            <message clientCredentialType="Certificate" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:2835/Service1.svc" binding="wsHttpBinding"
          bindingConfiguration="WSHttpBinding_IService1" contract="IService1"
          name="WSHttpBinding_IService1" behaviorConfiguration="bahaveDemo">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
    </client>

    <!--begin 需手动添加添加-->
    <behaviors>
      <endpointBehaviors>
        <behavior name="bahaveDemo">
          <clientCredentials>
            <clientCertificate findValue="Client" storeName="My" storeLocation="LocalMachine" x509FindType="FindBySubjectName"/>
            <serviceCertificate>
              <authentication certificateValidationMode="None"/>
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <!--end 需手动添加添加-->
  </system.serviceModel>
</configuration>